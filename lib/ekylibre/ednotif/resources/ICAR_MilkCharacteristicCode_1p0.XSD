	<!-- ====================================================================== -->
	<!-- =====  Type Definitions					      ===== -->
	<!-- ====================================================================== -->
	<!-- =====  Type Definition: Milk Characteristic Type		      ===== -->
	<!-- ====================================================================== -->
	<xsd:simpleType name="MilkCharacteristicCodeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="SCC">
				<xsd:annotation>
					<xsd:documentation>
						<ccts:Name>SCC</ccts:Name>
						<ccts:Definition>Somatic cell count</ccts:Definition>
