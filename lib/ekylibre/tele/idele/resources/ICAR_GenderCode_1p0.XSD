<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" xmlns:clmICARGenderCode="urn:it:icar:ade:codelist:GenderCode:1.0" targetNamespace="urn:it:icar:ade:codelist:GenderCode:1.0" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
