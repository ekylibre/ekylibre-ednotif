	<xsd:element name="GenderCode" type="clmICARGenderCode:GenderCodeType"/>
	<!-- ====================================================================== -->
	<!-- =====  Type Definitions					      ===== -->
	<!-- ====================================================================== -->
	<!-- =====  Type Definition: Gender Animal Type		      ===== -->
	<!-- ====================================================================== -->
	<xsd:simpleType name="GenderCodeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="F">
				<xsd:annotation>
					<xsd:documentation>
						<ccts:Name>F</ccts:Name>
						<ccts:Definition>Female</ccts:Definition>
					</xsd:documentation>
